# how to ns: use keywords, not symbols

--Input
(ns com.example.my-application
  (require
   [clojure.string :as string])
  (import
   (java.util Date)))
--Input

--Expected
(ns com.example.my-application
  (:require
   [clojure.string :as string])
  (:import
   (java.util Date)))
--Expected

# alphabetically sort ns requires

--Input
(ns foo
  (:require b [c])
  (:require a))
--Input

--Expected
(ns foo
  (:require
   [a]
   [b]
   [c]))
--Expected

# sort main clauses: refer-clojure, require, import

--Input
(ns foo
  (:require b c a)
  (:import (java.util Date))
  (:refer-clojure :exclude [get])
  )
--Input

--Expected
(ns foo
  (:refer-clojure :exclude [get])
  (:require
   [a]
   [b]
   [c])
  (:import
   (java.util Date)))
--Expected
