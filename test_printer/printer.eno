# Simple Indentation

--Input
(def aaa
(bbb))
--Input

--Expected
(def aaa
  (bbb))
--Expected

# Multiple Indentation Levels

--Input
(def (aaa)
      (bbb
(ccc)))
--Input

--Expected
(def (aaa)
  (bbb
    (ccc)))
--Expected

# Close Wrapping Parens

--Input
(def aaa
(bbb)
)
--Input

--Expected
(def aaa
  (bbb))
--Expected

# Do not cross the next indentation level

--Input
(defn foo
  ([a]
    (foo a ""))
  ([a b]
    (str a "-" b)))
--Input

--Expected
(defn foo
  ([a]
   (foo a ""))
  ([a b]
   (str a "-" b)))
--Expected

# Preserve commas

--Input
{:a "a", :b "b"}
--Input

--Expected
{:a "a", :b "b"}
--Expected

# TODO: Trim trailing whitespace

--Input
(def aaa)
--Input

--Expected
(def aaa)
--Expected
